<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pc.kotlinroomlivedata">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="foreignkeyconcept.MovieDirector">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".DataListActivity"></activity>
        <activity android:name="roomwithlivedata.ListActivity"></activity>
        <activity android:name="roomwithlivedata.TestActivity"></activity>
        <activity android:name="roomwithlivedata.UserDepartmentActivity"></activity>
        <activity android:name="roomwithlivedata.DepartmentListActivity"></activity>

    </application>

</manifest>